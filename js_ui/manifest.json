{
  "name": "Multicopter Config",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",

  "app": {
    "background": {
      "scripts": [ "main.js" ],
      "persistent": true
    }
  },

  "permissions": [
    "serial",
    "storage",
    "fileSystem",
    "fileSystem.write",
    "commands"
  ]

